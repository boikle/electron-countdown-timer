{
  "name": "electron-countdown-timer",
  "version": "1.0.0",
  "description": "A simple count down timer built with electron.js",
  "main": "./src/main/index.js",
  "private": true,
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . countdown-timer --overwrite --asar=true --platform=linux --arch=x64 --icon=build/icon.png --prune=true --out=dist",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/roikle/electron-countdown-timer.git"
  },
  "keywords": [
    "electron.js",
    "countdown",
    "timer"
  ],
  "author": "Robert Oikle",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/roikle/electron-countdown-timer/issues"
  },
  "homepage": "https://github.com/roikle/electron-countdown-timer#readme",
  "devDependencies": {
    "electron": "^9.0.4",
    "electron-packager": "^15.0.0",
    "eslint": "^7.3.0",
    "jsdom": "^16.2.2",
    "mocha": "^8.1.3"
  }
}
